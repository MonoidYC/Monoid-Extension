{
  "name": "monoid-visualize",
  "displayName": "monoid-visualize",
  "publisher": "monoid",
  "description": "Monoid's visualize extension that can convert code into nodes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onUri"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "monoid-visualize.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "monoid-visualize.visualizeAllCode",
        "title": "Monoid: Visualize All Code",
        "icon": "$(graph)"
      },
      {
        "command": "monoid-visualize.openGraphPanel",
        "title": "Monoid: Open Graph Panel",
        "icon": "$(preview)"
      },
      {
        "command": "monoid-visualize.refreshGraph",
        "title": "Monoid: Refresh Graph",
        "icon": "$(refresh)"
      },
      {
        "command": "monoid-visualize.testLM",
        "title": "Monoid: Test VS Code LM",
        "icon": "$(beaker)"
      },
      {
        "command": "monoid-visualize.generateTest",
        "title": "Monoid: Generate E2E Test",
        "icon": "$(add)"
      },
      {
        "command": "monoid-visualize.runAllTests",
        "title": "Monoid: Run All Tests",
        "icon": "$(run-all)"
      },
      {
        "command": "monoid-visualize.runTestFile",
        "title": "Monoid: Run Test File",
        "icon": "$(play)"
      },
      {
        "command": "monoid-visualize.runTest",
        "title": "Monoid: Run Test",
        "icon": "$(play)"
      },
      {
        "command": "monoid-visualize.debugTest",
        "title": "Monoid: Debug Test",
        "icon": "$(debug-alt)"
      },
      {
        "command": "monoid-visualize.refreshTests",
        "title": "Monoid: Refresh Tests",
        "icon": "$(refresh)"
      },
      {
        "command": "monoid-visualize.openTestFile",
        "title": "Monoid: Open Test File",
        "icon": "$(go-to-file)"
      },
      {
        "command": "monoid-visualize.openPlaywrightUI",
        "title": "Monoid: Open Playwright UI",
        "icon": "$(browser)"
      },
      {
        "command": "monoid-visualize.showTestReport",
        "title": "Monoid: Show Test Report",
        "icon": "$(output)"
      },
      {
        "command": "monoid-visualize.regenerateTest",
        "title": "Monoid: Regenerate Test",
        "icon": "$(sync)"
      },
      {
        "command": "monoid-visualize.generateAllTests",
        "title": "Monoid: Generate Tests for Entire App",
        "icon": "$(checklist)"
      },
      {
        "command": "monoid-visualize.generateTestsFromCodeGraph",
        "title": "Monoid: Generate Tests from Code Graph",
        "icon": "$(graph)"
      },
      {
        "command": "monoid-visualize.benchmarkTestGeneration",
        "title": "Monoid: Benchmark Test Generation Methods",
        "icon": "$(dashboard)"
      },
      {
        "command": "monoid-visualize.openTestPanel",
        "title": "Monoid: Open Test Panel",
        "icon": "$(beaker)"
      },
      {
        "command": "monoid-visualize.deleteAllTests",
        "title": "Monoid: Delete All Tests",
        "icon": "$(trash)"
      },
      {
        "command": "monoid-visualize.syncTestsToSupabase",
        "title": "Monoid: Sync Tests to Supabase",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "monoid-visualize.signIn",
        "title": "Monoid: Sign In with GitHub",
        "icon": "$(sign-in)"
      },
      {
        "command": "monoid-visualize.signOut",
        "title": "Monoid: Sign Out",
        "icon": "$(sign-out)"
      },
      {
        "command": "monoid-visualize.pasteSession",
        "title": "Monoid: Paste Session Token (Dev)",
        "icon": "$(key)"
      },
      {
        "command": "monoid-visualize.generateCodeNode",
        "title": "Monoid: Generate Code Node from Natural Language",
        "icon": "$(sparkle)"
      },
      {
        "command": "monoid-visualize.generateTestFromNaturalLanguage",
        "title": "Monoid: Generate Test from Natural Language",
        "icon": "$(beaker)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "monoid-visualize",
          "title": "Monoid Visualize",
          "icon": "$(graph)"
        }
      ]
    },
    "views": {
      "monoid-visualize": [
        {
          "type": "webview",
          "id": "monoid-visualize.graphView",
          "name": "Code Graph"
        },
        {
          "id": "monoid-visualize.playwrightTests",
          "name": "Playwright Tests"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "monoid-visualize.visualizeAllCode",
          "when": "view == monoid-visualize.graphView",
          "group": "navigation"
        },
        {
          "command": "monoid-visualize.refreshGraph",
          "when": "view == monoid-visualize.graphView",
          "group": "navigation"
        },
        {
          "command": "monoid-visualize.generateTest",
          "when": "view == monoid-visualize.playwrightTests",
          "group": "navigation@1"
        },
        {
          "command": "monoid-visualize.generateAllTests",
          "when": "view == monoid-visualize.playwrightTests",
          "group": "navigation@2"
        },
        {
          "command": "monoid-visualize.generateTestsFromCodeGraph",
          "when": "view == monoid-visualize.playwrightTests",
          "group": "navigation@3"
        },
        {
          "command": "monoid-visualize.runAllTests",
          "when": "view == monoid-visualize.playwrightTests",
          "group": "navigation@4"
        },
        {
          "command": "monoid-visualize.refreshTests",
          "when": "view == monoid-visualize.playwrightTests",
          "group": "navigation@5"
        },
        {
          "command": "monoid-visualize.openPlaywrightUI",
          "when": "view == monoid-visualize.playwrightTests",
          "group": "navigation@6"
        },
        {
          "command": "monoid-visualize.openTestPanel",
          "when": "view == monoid-visualize.playwrightTests",
          "group": "navigation@7"
        }
      ],
      "view/item/context": [
        {
          "command": "monoid-visualize.runTestFile",
          "when": "view == monoid-visualize.playwrightTests && viewItem == testFile",
          "group": "inline@1"
        },
        {
          "command": "monoid-visualize.runTestFile",
          "when": "view == monoid-visualize.playwrightTests && viewItem == testFile",
          "group": "1_run@1"
        },
        {
          "command": "monoid-visualize.debugTest",
          "when": "view == monoid-visualize.playwrightTests && viewItem == testFile",
          "group": "1_run@2"
        },
        {
          "command": "monoid-visualize.regenerateTest",
          "when": "view == monoid-visualize.playwrightTests && viewItem == testFile",
          "group": "2_manage@1"
        },
        {
          "command": "monoid-visualize.runTest",
          "when": "view == monoid-visualize.playwrightTests && viewItem == test",
          "group": "inline@1"
        },
        {
          "command": "monoid-visualize.runTest",
          "when": "view == monoid-visualize.playwrightTests && viewItem == test",
          "group": "1_run@1"
        },
        {
          "command": "monoid-visualize.debugTest",
          "when": "view == monoid-visualize.playwrightTests && viewItem == test",
          "group": "1_run@2"
        }
      ],
      "editor/context": [
        {
          "command": "monoid-visualize.generateTest",
          "when": "resourceExtname == .js || resourceExtname == .jsx || resourceExtname == .ts || resourceExtname == .tsx",
          "group": "monoid@1"
        }
      ]
    },
    "configuration": {
      "title": "Monoid Visualize",
      "properties": {
        "monoid-visualize.supabaseUrl": {
          "type": "string",
          "default": "",
          "description": "Supabase project URL"
        },
        "monoid-visualize.supabaseAnonKey": {
          "type": "string",
          "default": "",
          "description": "Supabase anon key"
        },
        "monoid-visualize.workspaceSlug": {
          "type": "string",
          "default": "",
          "description": "Workspace slug for storing code graphs"
        },
        "monoid-visualize.webAppUrl": {
          "type": "string",
          "default": "https://monoid-dashboard.vercel.app",
          "description": "URL of the Monoid dashboard to embed in the graph panel"
        },
        "monoid-visualize.githubOwner": {
          "type": "string",
          "default": "",
          "description": "GitHub repository owner (e.g., 'tanujsiripurapu')"
        },
        "monoid-visualize.githubRepo": {
          "type": "string",
          "default": "",
          "description": "GitHub repository name (e.g., 'my-app')"
        },
        "monoid-visualize.githubBranch": {
          "type": "string",
          "default": "main",
          "description": "GitHub branch name for generating links"
        },
        "monoid-visualize.testBaseUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "Base URL for E2E tests"
        },
        "monoid-visualize.testOutputDir": {
          "type": "string",
          "default": "e2e",
          "description": "Directory for generated Playwright tests"
        },
        "monoid-visualize.testHeadedMode": {
          "type": "boolean",
          "default": true,
          "description": "Run tests in headed mode (visible browser) by default"
        },
        "monoid-visualize.testVideoMode": {
          "type": "string",
          "enum": ["on", "off", "retain-on-failure", "on-first-retry"],
          "default": "retain-on-failure",
          "description": "Playwright video recording mode: 'on' (record all), 'off' (no videos), 'retain-on-failure' (keep videos only for failed tests), 'on-first-retry' (record on retries)"
        },
        "monoid-visualize.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API key for generating node summaries. Get one at https://aistudio.google.com/apikey"
        },
        "monoid-visualize.llmProvider": {
          "type": "string",
          "enum": ["copilot", "gemini"],
          "default": "gemini",
          "description": "LLM provider for test generation"
        },
        "monoid-visualize.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API key for test generation"
        },
        "monoid-visualize.geminiModel": {
          "type": "string",
          "default": "gemini-3-flash-preview",
          "description": "Gemini model to use (e.g., gemini-2.0-flash, gemini-1.5-pro)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.2"
  }
}
